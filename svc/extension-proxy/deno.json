{
    "tasks": {
        "dev": "deno serve -A main.ts",
        "run": "deno serve --allow-env=HMAC_SECRET,PROXY_BASE_URL --allow-net --cached-only --parallel main.ts"
    },
    "imports": {
        "@libs/xml": "jsr:@libs/xml@^6.0.4",
        "@std/assert": "jsr:@std/assert@1",
        "@std/encoding": "jsr:@std/encoding@^1.0.10"
    },
    "fmt": {
        "indentWidth": 4,
        "singleQuote": true,
        "lineWidth": 100,
        "trailingCommas": "onlyMultiLine"
    }
}
